{
  "_version": "1.1.0",
  "sap.app": {
  	"_version": "1.1.0",
  	"id": "sap.clr",
  	"type": "application",
  	"i18n": "i18n/i18n.properties",
  	"title": "{{appTitle}}",
  	"description": "{{appDescription}}",
  	"applicationVersion": {
  	  "version": "1.0.0"
    }
  },
  "sap.ui": {
  	"_version": "1.1.0",
  	"technology": "UI5",
  	"deviceTypes": {
  	  "desktop": true,
  	  "tablet": true,
  	  "phone": true
  	},
  	"supportedThemes": [
  	  "sap_bluecrystal"
  	]
  },
  "sap.ui5": {
  	"_version": "1.1.0",
  	"rootView": {
        "viewName": "sap.clr.view.App",
        "type": "JS"
    },
  	"dependencies": {
  	  "minUI5Version": "1.32",
  	  "libs": {
  		    "sap.m": {}
  	  }
  	},
    "resources": {
      "css": [
        {
          "uri": "style.css"
        }
      ]
    },
  	"models": {
  	  "i18n": {
    		"type": "sap.ui.model.resource.ResourceModel",
    		"settings": {
    		  "bundleName": "sap.clr.i18n.i18n"
    		}
  	  }
  	},
    "routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "sap.clr.view",
        "viewType": "JS",
				"clearTarget": false,
        "transition": "show",
        "controlId": "App",
        "controlAggregation": "pages",
        "bypassed": {
           "target": "notfound"
        }
			},
			"routes": [
        {
  				"pattern": "home",
  				"name": "home",
          "target": "home"
  			},
        {
  				"pattern": "",
  				"name": "login",
          "target": "login"
  			},
        {
  				"pattern": "landscapes",
  				"name": "landscapes",
          "target": "landscapes"
  			},
        {
  				"pattern": "landscape/{id}",
  				"name": "landscape",
          "target": "landscape"
  			},
        {
  				"pattern": "landscape/{id}/external/new",
  				"name": "landscapeExternalNew",
          "target": "landscapeExternalNew"
  			},
        {
  				"pattern": "landscape/{id}/external/{reportId}",
  				"name": "landscapeExternal",
          "target": "landscapeExternal"
  			},
        {
  				"pattern": "users",
  				"name": "users",
          "target": "users"
  			},
        {
  				"pattern": "gsc",
  				"name": "gsc",
          "target": "gsc"
  			}
      ],
      "targets": {
          "login": {
             "viewName": "Login"
          },
          "home": {
             "viewName": "Home"
          },
          "landscapes": {
             "viewName": "Landscapes"
          },
          "landscape": {
             "viewName": "Landscape"
          },
          "landscapeExternalNew": {
             "viewName": "LandscapeExternalNew"
          },
          "landscapeExternal": {
             "viewName": "LandscapeExternal"
          },
          "users": {
             "viewName": "Users"
          },
          "gsc": {
             "viewName": "GSC"
          },
          "notfound": {
             "viewName": "NotFound",
             "viewType": "XML"
          }
       }
		}
  }
}
